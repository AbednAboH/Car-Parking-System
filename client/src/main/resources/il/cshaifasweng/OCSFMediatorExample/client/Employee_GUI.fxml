<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<Scene xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.Employee_GUI_Controller">
    <AnchorPane prefHeight="422.0" prefWidth="477.0">
      <children>
         <TableView fx:id="Table" layoutX="45.0" layoutY="111.0" onSort="#Table" prefHeight="200.0" prefWidth="300.0">
           <columns>
             <TableColumn fx:id="row" prefWidth="75.0" text="row" />
             <TableColumn fx:id="col" prefWidth="75.0" text="col" />
               <TableColumn fx:id="depth" prefWidth="75.0" text="depth" />
               <TableColumn fx:id="available" prefWidth="75.0" text="available" />
           </columns>
         </TableView>
         <Button fx:id="intialize" layoutX="45.0" layoutY="63.0" mnemonicParsing="false" onAction="#intializeBtn" text="intialize" />
         <Label layoutX="45.0" layoutY="351.0" text="row" />
         <TextField fx:id="rowLabel" layoutX="39.0" layoutY="383.0" onAction="#SaveSpot" prefHeight="25.0" prefWidth="46.0" />
         <Label layoutX="114.0" layoutY="351.0" text="col" />
         <Label layoutX="179.0" layoutY="351.0" text="depth" />
         <TextField fx:id="colLabel" layoutX="99.0" layoutY="383.0" onAction="#SaveSpot" prefHeight="25.0" prefWidth="46.0" />
         <TextField fx:id="depthLabel" layoutX="172.0" layoutY="383.0" onAction="#SaveSpot" prefHeight="25.0" prefWidth="46.0" />
         <Button fx:id="SaveSpot" layoutX="258.0" layoutY="383.0" mnemonicParsing="false" onAction="#SaveSpot" text="SaveSpot" />
         <Button fx:id="DirectToAvailblePark" layoutX="117.0" layoutY="63.0" mnemonicParsing="false" onAction="#DirectToAvailblePark" text="direct to availble park" />
      </children></AnchorPane>
</Scene>
